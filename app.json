{
  "expo": {
    "name": "Feed.Me",
    "description": "This Application provides 'game rooms' for all users to ask other users about their Feedback or Opinions",
    "slug": "Feed.Me",
    "platforms": ["ios", "android", "web"],

    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.jonas0608.RoomsForFeedbackAndOpinions",
      "permissions": []
    }
  }
}
